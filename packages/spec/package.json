    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "deepmerge": "^4.2.2",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-jsx-plus": "^0.1.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "require-all": "^3.0.0",
    "stylelint-scss": "^3.18.0"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "rax": "^1.2.2",
    "react": "^17.0.2",
